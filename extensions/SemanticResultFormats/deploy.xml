<?xml version="1.0" encoding="UTF-8"?>
<deploydescriptor>
    <global>
        <version>150</version>
        <id>semanticresultformats</id>
        <vendor>Ontoprise GmbH</vendor>
        <instdir>extensions/SemanticResultFormats</instdir>
        <description>
            Semantic Result Formats provides several new result printer for ASK (and SPARQL)
        </description>
        <dependencies>
            <dependency from="140" to="150">smw</dependency>
        </dependencies>
    </global>
    <codefiles></codefiles>
    <wikidumps>
        <!-- empty -->
    </wikidumps>
    <resources>
        <!-- empty -->
    </resources>
    <configs>
        <precedes ext="smw" />

        <new>
            <patch file="extensions/SemanticResultFormats/patch.txt" />
            <require name="srf"
                file="extensions/SemanticResultFormats/SRF_Settings.php" />
     
        </new>
        <uninstall>
           
        </uninstall>
    </configs>
</deploydescriptor>