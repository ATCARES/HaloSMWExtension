<?xml version="1.0" encoding="UTF-8"?>
<deploydescriptor>
    <global>
        <version>190</version>
        <id>semanticforms</id>
        <vendor>Ontoprise GmbH / Yaron Koren</vendor>
        <instdir>extensions/SemanticForms</instdir>
        <description>
            Store richmedia files in our your wiki
        </description>
        <dependencies>
            <dependency from="1120" to="1169">MW</dependency>
            <dependency from="142" to="150">smw</dependency>
        </dependencies>
    </global>
    <codefiles>

        <file loc="extensions/SemanticForms/deploy.xml" />
        <!-- TODO: add all others -->
    </codefiles>
    <wikidumps>
        <!-- empty -->
    </wikidumps>
    <resources>
        <!-- empty -->
    </resources>

    <configs>
    <successor ext="smw" />
        <new>
            <require name="sf_init"
                file="extensions/SemanticForms/includes/SF_Settings.php" />
            <patch file="extensions/SemanticForms/patch.txt" />
        </new>
            <uninstall>
        <patch file="extensions/SemanticForms/patch.txt" />
    </uninstall>
    </configs>

</deploydescriptor>

